    <div class="main-navbar conner-style style-2 position-fixed">
        <div class="container padding-none">
            <div class="row">
                <div class="col-md-12">
                    <nav class="navbar navbar-default">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand dis-none" href="#">
                                <img src="~/Assets/img/logo-black.png" alt="">
                            </a>
                            <a class="navbar-brand dis-block" href="#">
                                <img src="~/Assets/img/logo-black.png" alt="">
                            </a>
                        </div>

                        <!-- INICIO CLIENTE-->


                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1" data-hover="dropdown" data-animations-delay="1.8s" data-animations="fadeInUp">
                            <ul class="nav navbar-nav bg-none navbar-right style-3">
                                <li class="dropdown active">
                                    <a href="@Url.Action("Index", "Home")" class="dropdown-toggle" role="button">Inicio</a>
                                </li>


                                @{ if (Session["username"] != null)
                                    {

                                        MvcAuth.Models.Ecomerce2019Entities db = new MvcAuth.Models.Ecomerce2019Entities();
                                        Dictionary<string, int> navbar = new Dictionary<string, int>();

                                        List<MvcAuth.Models.tb_Categoria> Categorias = db.tb_Categoria.ToList();

                                        foreach (MvcAuth.Models.tb_Categoria cat in Categorias)
                                        {
                                            navbar.Add(cat.nomCtegoria, cat.idCategoria);
                                        }

                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><span data-hover="Depertment">Especialidad</span></a>
                                            <ul class="dropdown-menu">
                                                @{int count = 0;
                                                    foreach (KeyValuePair<string, int> nav in navbar)
                                                    {

                                                        <li>
                                                            @if (count < 10)
                                                            {

                                                                <a href="@Url.Action("findEspe", "Doctor", new {espe = nav.Key})">@nav.Key<span><i class="ion-ios-arrow-right"></i></span></a>

                                                            }

                                                        </li>

                                                        count++;
                                                    }



                                                    @*INT*@}

                                            </ul>
                                        </li>







                                        <li class="dropdown">
                                            <a href="@Url.Action("Index", "Doctor")" class="dropdown-toggle" role="button">
                                                Doctores
                                            </a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Index", "WishList")" class="dropdown-toggle" role="button">
                                                Favoritos
                                            </a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="@Url.Action("Contact", "Home")" class="dropdown-toggle" role="button">
                                                Contáctenos
                                            </a>
                                        </li>
                                        <li>
                                            <div class="dropdown-buttons">
                                                <div class="btn-group menu-search-box dropdown">
                                                    <button type="button" class="btn  dropdown-toggle" id="header-drop-3" role="button" aria-haspopup="true" aria-expanded="false"><i class="icon icon-Search"></i></button>
                                                    <ul class="dropdown-menu-right dropdown-menu cart dropdown-animation" aria-labelledby="header-drop-4">
                                                        <li>
                                                            @using (Html.BeginForm("findEspe", "Doctor", FormMethod.Get, new { @class = "search-box", @role = "search" }))
                                                            {
                                                                <div class="form-group" style="margin:10px; ">
                                                                    <input name="espe" type="text" class="form-control" style="border: 2px dotted RGBA(128, 128, 128, 0.3);border-radius:5px;height:50px;" placeholder="Buscar especialidad..">
                                                                </div>

                                                            }
                                                        </li>
                                                    </ul>
                                                </div>
                                                <div class="btn-group dropdown">
                                                    <button type="button" class="btn dropdown-toggle" id="header-drop-4" aria-haspopup="true" aria-expanded="false">  <a href="@Url.Action("Index", "MyCart")"> <i class="icon icon-ShoppingCart"></i><span class="menu-cart">.</span></a></button>


                                                </div>

                                            </div>


                                        </li>

                                    }
                                    else
                                    {
                                        <li class="dropdown">
                                            <a href="@Url.Action("RegisterDoctor", "Doctor")" class="dropdown-toggle" role="button">
                                                Información
                                            </a>
                                        </li>

                                        <li class="dropdown">
                                            <a href="@Url.Action("Contact", "Home")" class="dropdown-toggle" role="button">
                                                Contáctenos
                                            </a>
                                        </li>

                                    }

                                }




                            </ul>
                        </div>

                        <!-- FIN CLIENTE-->


                    </nav>
                </div>
            </div>
        </div>
