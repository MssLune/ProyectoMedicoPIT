                                    <dd>
                                        @Html.DisplayFor(model => model.caracteristica4)
                                    </dd>

                                    <dt style="width:400px; padding-right:80px;">
                                        EXPERIENCIA II
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.caracteristica5)
                                    </dd>




                                    <dt style="width:400px; padding-right:80px;">
                                        EXPERIENCIA III
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.caracteristica6)
