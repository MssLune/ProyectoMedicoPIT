                                    <dt style="width:400px; padding-right:80px;">
                                        PORCENTAJE MEDICINA GENERAL
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.porc3)
                                    </dd>

                                    <dt style="width:400px; padding-right:80px;">
                                        PORCENTAJE MEDICINA GENERAL
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.porc4)
                                    </dd>


                                </dl>
                            </div>
                        </div>
                    </div>
                    <div class="row m-t-30">
                        <button class="au-btn au-btn-icon au-btn--small" style="background-color:floralwhite; margin-left:15px;">
                            <a href="@Url.Action("Index", "Home", new {  })">
                                REGRESAR
                            </a>
                        </button>

                       

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

